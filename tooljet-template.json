    
{
  "version": "2",
  "templates": [     
{
      "type": 2,
      "title": "Tooljet",
      "description": "OExtensible low-code framework for building business applications.",
      "categories": ["low-code"],
      "platform": "linux",
      "logo": "https://portainer-io-assets.sfo2.digitaloceanspaces.com/logos/ubuntu.png",
      "repository": {
        "url": "https://github.com/SAVE-POlNT/goodcontainers",
        "stackfile": "tooljet/docker-compose.yaml"
      },
      "env": [
        {
          "name": "TOOLJET_HOST",
          "label": "server host",
          "default": "http://localhost:8082",
          "preset": true
        },
                {
          "name": "LOCKBOX_MASTER_KEY",
          "label": "replace_with_lockbox_master_key",
          "default": "replace_with_lockbox_master_key",
          "preset": true
        },
                {
          "name": "SECRET_KEY_BASE",
          "label": "replace_with_secret_key_base",
          "default": "replace_with_secret_key_base",
          "preset": true
        },
                {
          "name": "PG_DB",
          "label": "postgres database",
          "default": "postgres",
          "preset": true
        },
                {
          "name": "PG_USER",
          "label": "postgres user",
          "default": "postgres",
          "preset": true
        },
         {
          "name": "PG_HOST",
          "label": "postgres host",
          "default": "postgres",
          "preset": true
        },
        {
          "name": "PG_PASS",
          "label": "postgress password",
          "default": "http://localhost:8082",
          "preset": true
        },
          {
          "name": "DEFAULT_FROM_EMAIL",
          "label": "default from email",
          "default": "hello@tooljet.io"

        },
         {
          "name": "SMTP_USERNAME",
          "label": "smtp user"

        },
         {
          "name": "SMTP_PASSWORD",
          "label": "smtp password"

        },
         {
          "name": "SMTP_DOMAIN",
          "label": "SMTP_DOMAIN"

        },
        {
          "name": "SMTP_PORT",
          "label": "SMTP_PORT"

        },
        
          {
          "name": "DISABLE_SIGNUPS",
          "label": "disable signups"

        }
      ]
    },]}
